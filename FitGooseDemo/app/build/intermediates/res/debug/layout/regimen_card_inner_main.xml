<?xml version="1.0" encoding="utf-8"?>
<it.gmariotti.cardslib.library.prototypes.LinearListView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/card_inner_base_main_cardwithlist"
    android:orientation="vertical"
    android:layout_marginLeft="0dp"
    android:layout_marginRight="0dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp"
        android:layout_gravity="center_vertical"
        android:layout_height="wrap_content"
        android:weightSum="9">

        <TextView
            android:id="@+id/regimen_card_exercise_name"
            android:textSize="20sp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_weight="2.5"/>

        <TextView
            android:id="@+id/regimen_card_exercise_sets"
            android:textSize="20sp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="right"
            android:layout_weight="1"/>

        <TextView
            android:text="sets"
            android:textSize="20sp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:layout_weight="1.5"/>

        <TextView
            android:id="@+id/regimen_card_unit_one"
            android:textSize="18sp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_weight="2"/>

        <TextView
            android:id="@+id/regimen_card_unit_two"
            android:textSize="18sp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_weight="2"/>


    </LinearLayout>

</it.gmariotti.cardslib.library.prototypes.LinearListView><!-- From: file:/Users/guest/Documents/cs446/project/FitGooseDemo/app/src/main/res/layout/regimen_card_inner_main.xml -->